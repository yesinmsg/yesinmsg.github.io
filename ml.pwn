stock GetWeaponModel(weaponid)
{
	switch(weaponid)
	{
	    case 1:
	        return 331;

		case 2..8:
		    return weaponid+331;

		case 10..15:
			return weaponid+311;

		case 16..18:
		    return weaponid+326;

		case 22..29:
		    return weaponid+324;

		case 30,31:
		    return weaponid+325;

		case 32:
		    return 372;

		case 33..35:
		    return weaponid+324;

		case 46:
		    return 371;
	}
	return 0;
}

stock GetWeaponIDFromModel(modelid)
{
	switch(modelid)
	{
	    case 331:
	        return 1;

		case 2+331..8+331:
		    return modelid-331;

		case 10+311..15+311:
			return modelid-311;

		case 16+326..18+326:
		    return modelid-326;

		case 22+324..29+324:
		    return modelid-324;

		case 30+325,31+325:
		    return modelid-325;

		case 372:
		    return 32;

		case 33+324..35+324:
		    return modelid-324;

		case 371:
		    return 46;
	}
	return 0;
}


stock GetVehicleSpeed(vehicleid, get3d)
{
        new Float:x, Float:y, Float:z;
        GetVehicleVelocity(vehicleid, x, y, z);
        return SpeedCheck(x, y, z, 100.0, get3d);
}

stock ModifyVehicleSpeed(vehicleid,mph) //Miles Per Hour
{
        new Float:Vx,Float:Vy,Float:Vz,Float:DV,Float:multiple;
        GetVehicleVelocity(vehicleid,Vx,Vy,Vz);
        DV = floatsqroot(Vx*Vx + Vy*Vy + Vz*Vz);
        if(DV > 0) //Directional velocity must be greater than 0 (display strobes if 0)
        {
                multiple = ((mph + DV * 100) / (DV * 100)); //Multiplying DV by 100 calculates speed in MPH
                return SetVehicleVelocity(vehicleid,Vx*multiple,Vy*multiple,Vz*multiple);
        }
        return 0;
}

stock GetPlayerSpeed(playerid)
{
    new Float:ST[4];
    if(IsPlayerInAnyVehicle(playerid))
	GetVehicleVelocity(GetPlayerVehicleID(playerid),ST[0],ST[1],ST[2]);
	else GetPlayerVelocity(playerid,ST[0],ST[1],ST[2]);
    ST[3] = floatsqroot(floatpower(floatabs(ST[0]), 2.0) + floatpower(floatabs(ST[1]), 2.0) + floatpower(floatabs(ST[2]), 2.0)) * 150.3;
    return floatround(ST[3]);
}

