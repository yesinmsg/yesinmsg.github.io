forward CarSpawner(playerid,model);
public CarSpawner(playerid,model)
{
	if(IsPlayerInAnyVehicle(playerid)) SendClientMessage(playerid,red,"[Info] "COL_WHITE"У вас есть автомобиль!");
 	else
	{
    	new Float:x, Float:y, Float:z, Float:angle;
	 	GetPlayerPos(playerid, x, y, z);
	 	GetPlayerFacingAngle(playerid, angle);
		if(PlayerInfo[playerid][pCar] != -1) CarDeleter(PlayerInfo[playerid][pCar]);
	    new vehicleid=CreateVehicle(model, x, y, z, angle, random(128), random(128), -1);
		PutPlayerInVehicle(playerid, vehicleid, 0);
		SetVehicleVirtualWorld(vehicleid, GetPlayerVirtualWorld(playerid));
		LinkVehicleToInterior(vehicleid, GetPlayerInterior(playerid));
		ChangeVehicleColor(vehicleid,3,0);
        PlayerInfo[playerid][pCar] = vehicleid;
        if(GetVehicleModel(vehicleid) == 432) SetVehicleHealth(vehicleid, 10000);
        if(GetVehicleModel(vehicleid) == 601 || GetVehicleModel(vehicleid) == 528) SetVehicleHealth(vehicleid, 3000);
        SetVehicleNumberPlate(vehicleid, "{FF0000}Spawned");
	}
	return 1;
}

forward CarDeleter(vehicleid);
public CarDeleter(vehicleid)
{
    foreach(i)
	{
        new Float:Xp,Float:Yp,Float:Zp;
    	if(IsPlayerInVehicle(i, vehicleid))
		{
    	    RemovePlayerFromVehicle(i);
    	    GetPlayerPos(i,Xp,Yp,Zp);
        	SetPlayerPos(i,Xp,Yp+3,Zp);
	    }
	    SetVehicleParamsForPlayer(vehicleid,i,0,1);
	}
	for(new b=0;b<4;b++)DestroyObject(neon[vehicleid][b]),DestroyObject(wsneon[vehicleid][b]);
	if(veicolo_callsign_status[vehicleid] != 0)
	{
	DestroyDynamic3DTextLabel(veicolo_callsign_testo[vehicleid]);
	veicolo_callsign_status[vehicleid] = 0;
	}
	DestroyVehicle(vehicleid);
    //SetTimerEx("VehRes",1500,false,"i",vehicleid);
}

