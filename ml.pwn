		else if(strcmp(params,"down",true) == 0)	{
			TogglePlayerControllable(playerid,false); GetPlayerPos(playerid,Xj,Yj,Zj);	SetPlayerPos(playerid,Xj,Yj,Zj-5); SetCameraBehindPlayer(playerid); }
