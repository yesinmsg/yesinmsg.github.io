			px = floatstr(strtok(buffer,idx,','));
			py = floatstr(strtok(buffer,idx,','));
			pz = floatstr(strtok(buffer,idx,','));
			rx = floatstr(strtok(buffer,idx,','));
			ry = floatstr(strtok(buffer,idx,','));
			rz = floatstr(strtok(buffer,idx,')'));
