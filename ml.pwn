            SetPVarInt(playerid,"ADRENALIN",gettime() + 30);
            SetPVarInt(playerid,"RECUFF",gettime() + 15);
            SetPVarInt(playerid,"REARM",gettime() + 15);
            }
            switch(spec)
			{
	            case 0: PlayerInfo[playerid][pGJob] = 1,SpecInfo(playerid, PlayerInfo[playerid][pGJob]);
	            case 1: PlayerInfo[playerid][pGJob] = 2,SpecInfo(playerid, PlayerInfo[playerid][pGJob]);
	            case 2: PlayerInfo[playerid][pGJob] = 3,SpecInfo(playerid, PlayerInfo[playerid][pGJob]);
		        case 3: PlayerInfo[playerid][pGJob] = 4,SpecInfo(playerid, PlayerInfo[playerid][pGJob]);
	            case 4: PlayerInfo[playerid][pGJob] = 0;
			}

            if(GetPVarInt(playerid,"AdminInvisible") == 0)
            {
