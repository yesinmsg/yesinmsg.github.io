stock trim(str[], Char = ' ')
{
new string[128], pos[2] = {-1, -1};
for(new i, l = strlen(str); i != l; i++)
{
if(str[i] != Char && pos[0] == -1) pos[0] = i;
if(str[l - 1 - i] != Char && pos[1] == -1) pos[1] = l - i;
if(pos[0] != -1 && pos[1] != -1) break;
}
strmid(string, str, pos[0], pos[1]);
return string;
}

