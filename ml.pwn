    VoteKickCount = 0;
    CountKick     = 0;
    petro         = false;
    zapretkick    = true;
    TextDrawHideForAll(kickcounter);
    foreach(i) PlayerInfo[i][pVoted2] = false;
    KillTimer(kicktimer);
    }
	if(!GetPVarInt(playerid, #tazer_dest)) KillTimer(tazertimer[playerid]),TazedTimer(playerid);
	if(PlayerInfo[playerid][Frozen] == 1 || PlayerInfo[playerid][Fgoed] == 1) PlayerInfo[playerid][pJail] = 1;
    if(GetPVarInt(playerid,"CUFF") == 1 && reason == 1 || GetPVarInt(playerid,"CUFF") == 1 && reason == 2)
