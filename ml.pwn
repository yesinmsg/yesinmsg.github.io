			SendClientMessage(playerid, orange, "Функция: Устанавливает игроку вип-левел (0-Простой Аккаунт,1-Серебро,2-Золото,3-Премиум)");

	    	new player1, type, playername[MAX_PLAYER_NAME], adminname[MAX_PLAYER_NAME], string[256];
			player1 = strval(tmp);
			if(!strlen(tmp2)) return
