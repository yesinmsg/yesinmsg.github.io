    GetPlayerFacingAngle(playerid, angle);
    delivobj[playerid][3] = CreateObject(18728,x, y, z-2.4, 0, 0, 0);
    OnePlayAnim(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 0, 0, 0, 0);
    GetXYInFrontOfPlayer(playerid, x, y, 5);
    delivobj[playerid][0] = CreateObject(1681, x + (300 * -floatsin(-angle, degrees)), y + (300 * -floatcos(-angle, degrees)), z + 75.0, 0.0, 0.0, angle);
    MoveObject(delivobj[playerid][0], x + (300 * floatsin(-angle, degrees)), y + (300 * floatcos(-angle, degrees)), z + 75.0, 60.0);
    delivobj[playerid][1] = CreateObject(2935, x,y,z+75,0,0,0);
    delivobj[playerid][2] = CreateObject(18849, x,y,z+81, 0, 0, 0);
    AttachObjectToObject(delivobj[playerid][2], delivobj[playerid][1], 0.0, 0, 8, 0.0, 0.0, 0.0);
    delcallcar[playerid] = modelid;
    MoveObject(delivobj[playerid][1], x,y,z+2,3.0);
