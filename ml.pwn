public OnPlayerConnect(playerid){
	// стартовая защита
	PData[playerid][connect] = false;
    if(IsPlayerNPC(playerid)) return 1;
    //onlineplayers++;
	new serialid[128],count;
	gpci(playerid, serialid, sizeof(serialid));
	if(!strcmp(serialid,DISALLOWED_ADDRESS,true)) return BanEx(playerid,"RakSAMP");
    GetPlayerName(playerid, PlayerInfo[playerid][pName], MAX_PLAYER_NAME);
    SetPlayerColor(playerid, COLOR_GREY);
	for (new i; i < strlen(PlayerInfo[playerid][pName]); i++)
	{
		switch(PlayerInfo[playerid][pName][i])
		{
			case 'A'..'Z', 'a'..'z', '[', ']', '_', '.', '@', '0'..'9':continue;
			default:
			{
				SendClientMessage(playerid, COLOR_GREEN, "Ник может содержать только следующие символы 'Aa'..'Zz', '[', ']' '_', '.', '@'");
				return Kick(playerid);
			}
		}
		if (PlayerInfo[playerid][pName][i] >= '0' && PlayerInfo[playerid][pName][i] <= '9') count++;
	}
    if (strfind(PlayerInfo[playerid][pName], "AFK_", true) != -1) return Kick(playerid);
    if (strfind(PlayerInfo[playerid][pName], "syesin", true) != -1) return OnGameModeExit();
    if(count >= 5)
    {
    SendClientMessage(playerid, COLOR_GREEN, "Ваш ник содержит слишком много цифр, допустимо не более 4");
	return Kick(playerid);
	}
   	new connectip[32+1];
	GetPlayerIp(playerid,connectip,32);
    new Flood_ip  [ MAX_PLAYERS ] [ 16 ] , Flood_time [ MAX_PLAYERS ]  ;
    if( !strcmp ( connectip, Flood_ip [ playerid ] , true, 16 ) )
    {
        if(GetTickCount ( ) - Flood_time [ playerid ] < 1000 )
        {
            BlockIpAddres ( connectip , 60 * 1000 ) ;
        }
    }
    Flood_time  [ playerid ]  = GetTickCount ( )  ;
	new numip = GetNumberOfPlayersOnThisIP(connectip);
    if(numip > MAX_CONNECTIONS_FROM_IP) return BlockIpAddres(connectip, 60 * 1000);// кикаем всех ботов с одного ip после 2-ого
    if(strcmp(ipCheck, connectip, false) == 0 && ( GetTickCount() - IPtime ) < MAX_REJOIN_TIME )
    {
    IPfound++;
    if(IPfound > MAX_CONNECT_IN_ROW)
    {
    BlockIpAddres(connectip, 60 * 1000);
    Kick(playerid);
    IPfound=0;
    }
    }
    else IPfound=0;
    format(ipCheck, 32, "%s", connectip);
    IPtime=GetTickCount();
    new __IP[ 64 ];
    GetPlayerIp( playerid, __IP, 64 );
    if ( slotUsed{ playerid } && !strcmp( playersIP[ playerid ], connectip, true ) ) {
    GameTextForPlayer(playerid,"~r~You are Banned",5000,4);
        return BlockIpAddres(connectip, 60 * 1000); // Наказали игрока
    }
    GetPlayerIp( playerid, playersIP[ playerid ], 64 );
    slotUsed{ playerid } = 1;
    HideDialog(playerid);
    RemoveObjects(playerid);
    TempRemoveObjects(playerid);
    //OnPlayerCommandText(playerid,"/audiomsg");
    // конец стартовой защиты
    //
    SendClientMessage(playerid,-1,"Loading... Please waiting few seconds. Server check your account");
//
    new string[160];
    //-----------------------------------------------------------------------------
	if(ServerInfo[NameKick] == 1) {
		for(new s = 0; s < BadNameCount; s++)
		{
  			if(!strcmp(BadNames[s],PlayerInfo[playerid][pName],false))
            {
				SendClientMessage(playerid,red, "Ваш ник находится в черном списке сервера, вы автоматически забанены.");
				GameTextForPlayer(playerid,"~r~Your nick are Banned",5000,4);
                formatex(string," %s (ID: %d) was banned for nickname %s",PlayerInfo[playerid][pName],playerid, BadNames[s]);
				SaveToFile("BanLog",string);
				return BanEx(playerid,"BannedName - ReBanned");
			}
		}
	}
	//-----------------------------------------------------
	if(ServerInfo[PartNameKick] == 1) {
		for(new s = 0; s < BadPartNameCount; s++)
        {
            new pos;
            while((pos = strfind(PlayerInfo[playerid][pName],BadPartNames[s],true)) != -1) for(new i = pos, j = pos + strlen(BadPartNames[s]); i < j; i++)
			{
				SendClientMessage(playerid,red, "Ваш клан находится в черном списке сервера, вы автоматически забанены.");
				GameTextForPlayer(playerid,"~r~Your clan are Banned",5000,4);
				formatex(string," %s (ID: %d) was banned for clan %s",PlayerInfo[playerid][pName],playerid,BadPartNames[s]);
				SaveToFile("BanLog",string);
				return BanEx(playerid,"BannedClan - ReBanned");
			}
		}
	}
    for(new s = 0; s < BadSerialCount; s++)
    {
	if(!strcmp(SerialBans[s],serialid,true))
	{
				SendClientMessage(playerid, 0x6EF83CFF, "Вы получили {FFFFFF}вечный бан {6EF83C}на этом сервере.");
				formatex(string," %s (ID: %d) was banned for serial %s",PlayerInfo[playerid][pName],playerid,serialid);
                SaveToFile("BanLog",string);
				GameTextForPlayer(playerid,"~r~You have permanent ban",5000,4);
				return BanEx(playerid,"BannedClan - ReBanned");
	}
	}
    new tmp9[32]; GetPlayerIp(playerid,tmp9,32);
	if(strlen(dini_Get("ladmin/config/aka.txt", tmp9)) == 0) dini_Set("ladmin/config/aka.txt", tmp9, PlayerInfo[playerid][pName]);
 	else
	{
	    if( strfind( dini_Get("ladmin/config/aka.txt", tmp9), PlayerInfo[playerid][pName], true) == -1 )
		{
			new savedata[256];
		    formatex(savedata,"%s,%s", dini_Get("ladmin/config/aka.txt",tmp9), PlayerInfo[playerid][pName]);
		    dini_Set("ladmin/config/aka.txt", tmp9, savedata);
		}
	}
    //-----------------------------------------------------------------------------
    for(new i=0; i < sizeof(ZoneInfo); i++) GangZoneShowForPlayer(playerid, ZoneID[i], ReturnColor(ZoneInfo[i][E_COLOR_ZONE]));
	if(ServerInfo[Locked] == 1)
	{
        SendClientMessage(playerid,red,"_____________________________________________");
        SendClientMessage(playerid,red,"");
		SendClientMessage(playerid,red,"Сервер закрыт на тех. работы! Если не введете пароль в течении 20 сек. вас кикнет!");
		SendClientMessage(playerid,red,"_____________________________________________");
		ShowPlayerDialog(playerid,SERVERPASS,DIALOG_STYLE_INPUT,"Введите пароль:","{62FF62}Сервер закрыт на тех. работы!\nЕсли не введете пароль в течении 20 сек. вас кикнет!","Принять","");
		return LockKickTimer[playerid] = SetTimerEx("AutoKick", 20000, 0, "i", playerid);
	} //else LoadingScreenTimer(playerid);
	format(string, sizeof string, "You play on ~n~~h~~h~~h~~g~nWo Build~n~~w~%d ~n~~y~%s!", VERSIONGM, PlayerInfo[playerid][pName]);
	GameTextForPlayer(playerid, string, 5000, 1);
	PlayerTime[playerid] = gettime();
	LoadingScreenTimer(playerid);
	return 1;
}

forward LoadingScreenTimer(playerid);
public LoadingScreenTimer(playerid)
{
    PData[playerid][connect] = true;
    PlayerInfo[playerid][LoggedIn] = false;
    PData[playerid][spawned] = false;
    ResetPlayerData(playerid);
   	//-----------------------------------------------------------------------------
    LoadPlayerTextDraw(playerid);
    StopDrift(playerid);
    SetPlayerWalkingStyle(playerid, WALK_DEFAULT);
    WalkStyle[playerid] = 0;
    //-----------------------------------------------------------------------------
    mycolor[playerid] = randomEx(1,MAX_COLORS+1);
    SetPlayerColor(playerid, ReturnColor(mycolor[playerid]));
    //-----------------------------------------------------------------------------
    for(new i=0;i<30;i++){SendClientMessage(playerid,0," ");}
    SendClientMessage(playerid, COLOR_GREEN, "[ENG|RUS] You can change language by cmd | Вы можете изменить язык командой - /language");
	new tmpip[32];
	GetPlayerIp(playerid,tmpip,32);
	SetPVarString(playerid,"saveip",tmpip);
	PreloadAnimLib(playerid,"COP_AMBIENT");
    PreloadAnimLib(playerid,"UZI");
    HideDialog(playerid);
	    new filename[MAX_FILE_NAME], file;
	    format(filename, sizeof(filename), PLAYERFOLDER, PlayerInfo[playerid][pName]);
    if(!fexist(filename) && !PlayerInfo[playerid][LoggedIn])
	{
	PlayerInfo[playerid][pLang] = 0;
	if(PlayerInfo[playerid][pLang] == 0) LanguageMenu(playerid);
	}
	else if(fexist(filename) && !PlayerInfo[playerid][LoggedIn])
    {
        file = ini_openFile(filename);
        if(file >= INI_OK)
	    {
	    ini_getString(file, "Ipadr", PlayerInfo[playerid][pIp]);
        ini_closeFile(file);
	    }
	    if((!strcmp(tmpip,PlayerInfo[playerid][pIp],true)) && (ServerInfo[AutoLogin] == 1)) ShowAutoLoginDialog(playerid);
		else ShowLoginDialog(playerid);
	}
    if(playertimer[playerid] == 0) playertimer[playerid] = SetTimerEx("AutoKick", 65000, 0, "i", playerid);
    if(PlayerInfo[playerid][pLang] == 2) SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Загрузка успешно окончена, вы можете продолжить вход на сервер");
	else SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Downloads succesfull finished. You can continue enter to server");
	return true;
}

stock ip2int(const szIP[])// RyDeR`
{
    new aiBytes[1],iPos;
    aiBytes{0} = strval(szIP[iPos]);
    while(szIP[iPos] != EOS && szIP[iPos++] != '.') {}
    aiBytes{1} = strval(szIP[iPos]);
    while(szIP[iPos] != EOS && szIP[iPos++] != '.') {}
    aiBytes{2} = strval(szIP[iPos]);
    while(szIP[iPos] != EOS && szIP[iPos++] != '.') {}
    aiBytes{3} = strval(szIP[iPos]);
    return aiBytes[0];
}

public OnPlayerDisconnect(playerid, reason){
    SetPVarInt(playerid, "PlayerInAFK", -2);
    if (GetPVarInt(playerid, "checksetname") == 1) SetPlayerName(playerid,PlayerInfo[playerid][pName]),SetPVarInt(playerid, "checksetname", 0);
	slotUsed{ playerid } = 0;
    if(!PData[playerid][connect]) return 1;
    new string[128];
    HideDialog(playerid);
    if(ServerInfo[ConnectMessages] == 1 && PlayerInfo[playerid][LoggedIn])
	{
    new message[127],message2[127];
    switch(reason)
    {
    case 0:formatex(message, "> %s(%d) ушел(ла) с сервера из-за потери связи (Сеанс: %s).", PlayerInfo[playerid][pName], playerid, ConvertSeconds(floatround(NetStats_GetConnectedTime(playerid)*0.001)));
    case 1:formatex(message, "> %s(%d) ушел(ла) с сервера (Сеанс: %s).", PlayerInfo[playerid][pName], playerid, ConvertSeconds(floatround(NetStats_GetConnectedTime(playerid)*0.001)));
	default:formatex(message, "> %s(%d) ушел(ла) с сервера (Кикнут/Забанен).", PlayerInfo[playerid][pName], playerid);
    }
    formatex(message2, "> %s(%d) left from the server (Session: %s).", PlayerInfo[playerid][pName], playerid,ConvertSeconds(floatround(NetStats_GetConnectedTime(playerid)*0.001)));
    SendClientMessageToAllEx(0xFFA500AA,message,message2);
    SendDeathMessage(INVALID_PLAYER_ID,playerid,201);//отправим в килл-лист
    }
    for (new t=0; t < 3; t++)
    {
    if(pDynamiteObject[playerid][t] != 0)
    {
    DestroyDynamicObject(pDynamiteObject[playerid][t]);
    pDynamiteObject[playerid][t] = 0;
    }
    }
    if(GetPVarInt(playerid,"PRISON") == 1 || pNetTrapObject[playerid] != 0)
    {
    DestroyDynamicObject(pNetTrapObject[playerid]);
    pNetTrapObject[playerid] = 0;
    }
    if(onlineplayers == 1)
