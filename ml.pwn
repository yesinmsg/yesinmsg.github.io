stock ProxDetector(Float:radi, playerid, string[],col1,col2,col3,col4,col5)
{
	if(IsPlayerConnected(playerid))
	{
		new Float:posx, Float:posy, Float:posz;
		new Float:oldposx, Float:oldposy, Float:oldposz;
		new Float:tempposx, Float:tempposy, Float:tempposz;
		GetPlayerPos(playerid, oldposx, oldposy, oldposz);
		if(GetPlayerVirtualWorld(playerid) == DEBUGWORLD) SendClientMessage(playerid, col1, string);
		foreach (i)
		{
					if(GetPlayerVirtualWorld(playerid) == GetPlayerVirtualWorld(i) && !PMIgnore[i][playerid])
					{
						GetPlayerPos(i, posx, posy, posz);
						tempposx = (oldposx -posx);
						tempposy = (oldposy -posy);
						tempposz = (oldposz -posz);
						if (((tempposx < radi/16) && (tempposx > -radi/16)) && ((tempposy < radi/16) && (tempposy > -radi/16)) && ((tempposz < radi/16) && (tempposz > -radi/16)))
						{
							SendClientMessage(i, col1, string);
						}
						else if (((tempposx < radi/8) && (tempposx > -radi/8)) && ((tempposy < radi/8) && (tempposy > -radi/8)) && ((tempposz < radi/8) && (tempposz > -radi/8)))
						{
							SendClientMessage(i, col2, string);
						}
						else if (((tempposx < radi/4) && (tempposx > -radi/4)) && ((tempposy < radi/4) && (tempposy > -radi/4)) && ((tempposz < radi/4) && (tempposz > -radi/4)))
						{
							SendClientMessage(i, col3, string);
						}
						else if (((tempposx < radi/2) && (tempposx > -radi/2)) && ((tempposy < radi/2) && (tempposy > -radi/2)) && ((tempposz < radi/2) && (tempposz > -radi/2)))
						{
							SendClientMessage(i, col4, string);
						}
						else if (((tempposx < radi) && (tempposx > -radi)) && ((tempposy < radi) && (tempposy > -radi)) && ((tempposz < radi) && (tempposz > -radi)))
						{
							SendClientMessage(i, col5, string);
						}
					}
				}
				/*else
				{
					SendClientMessage(i, col1, string);
				}*/
	}
	return true;
}

SyncPlayer(playerid)
{
	if(IsPlayerInAnyVehicle(playerid)) return 1;
	if(!PData[playerid][spawned]) return 1;
	if(PlayerInfo[playerid][God] == true) return 1;
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING) return 1;
    if(GetPVarInt(playerid,"MOJNO") > gettime()) return SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Синхронизация доступна раз в 30 секунд!");
    SetPVarInt(playerid,"MOJNO",gettime() + 30);
    SyncPlayers[playerid] = true;
	new Float:HPp[2], Float:Posp[4], Intp, VirtualWorldp, SkinP, Money;
	GetPlayerHealth(playerid, HPp[0]);
	GetPlayerArmour(playerid, HPp[1]);
	SkinP = GetPlayerSkinID(playerid);
	Money = GetPlayerCash(playerid);
	GetPlayerPos(playerid, Posp[0], Posp[1], Posp[2]);
	GetPlayerFacingAngle(playerid, Posp[3]);
	Intp = GetPlayerInterior(playerid);
	VirtualWorldp = GetPlayerVirtualWorld(playerid);
	new Weapons[13][2];
	for(new i = 0; i < 13; i++) GetPlayerWeaponData(playerid, i, Weapons[i][0], Weapons[i][1]);
	SpawnPlayer(playerid);
	ClearAnimations(playerid);
	ac_ResetPlayerWeapons(playerid);
    ResetPlayerCash(playerid);
	SetPlayerPos(playerid, Posp[0], Posp[1], Posp[2]-0.4);
	SetPlayerFacingAngle(playerid, Posp[3]);
	SetPlayerHealth(playerid, HPp[0]);
	SetPlayerArmour(playerid, HPp[1]);
	SetPlayerInterior(playerid, Intp);
	SetPlayerSkin(playerid, SkinP);
	SetPlayerVirtualWorld(playerid, VirtualWorldp);
    GivePlayerCash(playerid,Money);
	for(new i = 0; i < 13; i++) ac_GivePlayerWeapon(playerid, Weapons[i][0], Weapons[i][1]);
	new str[100];
	formatex(str, "* Игрок %s (ID: %d) синхронизировался", PlayerInfo[playerid][pName],playerid);
	ProxDetector(30.0, playerid, str,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
	if (GetPVarInt(playerid,"AdminInvisible") == 0) SetPlayerChatBubble(playerid,"Синхронизировался",0x6495EDFF, 20.0, 5000);
	SendClientMessage(playerid,COLOR_GREY,"Вы успешно синхронизированы!");
	return 1;
}

stock SetPlayerColourAlpha(playerid, alpha)
{
	new r, g, b, a;
	HexToRGBA(GetPlayerColor(playerid), r, g, b, a);
	SetPlayerColor(playerid, RGBAToHex(r, g, b, alpha));
}
stock RGBAToHex(r, g, b, a)
{
	return (r<<24 | g<<16 | b<<8 | a);
}
stock HexToRGBA(colour, &r, &g, &b, &a)
{
	r = (colour >> 24) & 0xFF;
	g = (colour >> 16) & 0xFF;
	b = (colour >> 8) & 0xFF;
	a = colour & 0xFF;
}

stock encodepass(buf[]) {
	new length=strlen(buf);
    new s1 = 1;
    new s2 = 0;
    new n;
    for (n=0; n<length; n++)
    {
       s1 = (s1 + buf[n]) % 65521;
       s2 = (s2 + s1)     % 65521;
    }
    return (s2 << 16) + s1;
}


stock split(const strsrc[], strdest[][], delimiter)
{
new i, li;
new aNum;
new len;
while(i <= strlen(strsrc)){
if(strsrc[i]==delimiter || i==strlen(strsrc)){
len = strmid(strdest[aNum], strsrc, li, i, 128);
strdest[aNum][len] = 0;
li = i+1;
aNum++;
}
i++;
}
return 1;
}


