public OnPlayerPickUpPickup(playerid, pickupid)
{
	foreach(i)
	{
	if(pickupid == HealthPickup[i] && HealthPickup[i] != 0)
	{
	    new Float:health,health_amount=20+random(10);
	    GetPlayerHealth(playerid,health);
		if(health+health_amount > 100) SetPlayerHealth(playerid, 100);
		else SetPlayerHealth(playerid, health+health_amount);
		DestroyPickup(HealthPickup[i]);
		HealthPickup[i] = 0;
		return 1;
	}
	for(new p=0; p<DropLimit; p++)
    {
		if(pickupid == MoneyPickup[i][p] && MoneyPickup[i][p] != 0)
		{
		    GivePlayerCash(playerid, floatround(GetPVarInt(i,"MONEYDROP")/DropLimit));
		    DestroyPickup(MoneyPickup[i][p]);
		    MoneyPickup[i][p] = 0;
		}
	}
	}
	if(pickupid == bred)
	{
        new str[128];
		format(str, sizeof (str), "Игрок %s (ID: %d) был забанен системой за использование читов.", PlayerInfo[playerid][pName],playerid);
		SendClientMessageToAllEx(COLOR_RED, str);
		SaveToFile("BanLog",str);	Ban(playerid);
		return true;
	}
	return true;
}

