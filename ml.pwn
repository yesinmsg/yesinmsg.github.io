   SData[sTimeSec]++;
	if(SData[sTimeSec] == 60)
	{
				SData[sTimeMin]++;
				SData[sTimeSec]=0;
	}
	else if(SData[sTimeMin] == 60)
	{
				SData[sTimeHour]++;
