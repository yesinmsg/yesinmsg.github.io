dcmd_searchobject(playerid,params[]) {// *M*
	if(PlayerInfo[playerid][pAdmin] == 0) return SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Доступно только админам.");
	if(!strlen(params[0])) return SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Некорректно введен искомый объект. /searchobject [name/ID]");
	new items, arrSearch[125],nextline=0,string[1024];
	for (new i; i < strlen(params); i++)
	{
		switch(params[i])
		{
			case 'A'..'Z', 'a'..'z', '0'..'9', '_':continue;
			default: return SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"Название объекта может содержать только следующие символы 'Aa'..'Zz'");
		}
	}
	if(IsNumeric(params))
	{
	for(new z = 0; z < sizeof(ObjectList) && items < sizeof(arrSearch); z++)
	{
		if(ObjectList[z][oID] == strval(params))
		{
	 		arrSearch[items ++] = ObjectList[z][oID];
	 						if(nextline < 64) formatex(string,"%s%i(%d), ",string,ObjectList[z][oID],oID);
				else nextline=0,formatex(string,"%s\n%i(%d), ",string,ObjectList[z][oID],oID);
				nextline = nextline+strlen(ObjectList[z][oID]);
		}
	}
	}
	else
	{
	for(new z = 0; z < sizeof(ObjectList) && items < sizeof(arrSearch); z ++)
	{
		if(strfind(ObjectList[z][oName], params, true) != -1)
		{
	 		arrSearch[items ++] = ObjectList[z][oID];
	 						if(nextline < 64) formatex(string,"%s%i(%d), ",string,ObjectList[z][oID],oID);
				else nextline=0,formatex(string,"%s\n%i(%d), ",string,ObjectList[z][oID],oID);
				nextline = nextline+strlen(ObjectList[z][oID]);
		}
	}
	}

    if(!items) return SendClientMessage(playerid, COLOR_RED, "[Info] "COL_WHITE"ID/Name не был найден в базе данных или не существует, попробуйте другой");
    TempDialog(playerid,"Search Results",string,"Ок");
	return 1;
}

