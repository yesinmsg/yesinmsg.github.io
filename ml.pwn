public OnPlayerRequestSpawn(playerid)
{
    if(IsPlayerNPC(playerid)) return 1;
    if(!PlayerInfo[playerid][LoggedIn])
    {
    SendClientMessage(playerid,red,"*** Сначала авторизируйтесь");
    new filename[MAX_FILE_NAME];
    format(filename, sizeof(filename), PLAYERFOLDER, PlayerInfo[playerid][pName]);
   	if(!fexist(filename) && !PlayerInfo[playerid][LoggedIn]) ShowRegisterDialog(playerid);
	else if(fexist(filename) && !PlayerInfo[playerid][LoggedIn]) ShowLoginDialog(playerid);
    return 0;
	}
	if(PlayerInfo[playerid][LoggedIn])
	{
    StopAudioStreamForPlayer(playerid);
    if(PlayerInfo[playerid][pSetting][0] == 0)
	{
    for(new i=0;i<10;i++) TextDrawHideForPlayer(playerid, GradientText[i]),TextDrawShowForPlayer(playerid, GradientText[i]);
    TextDrawHideForPlayer(playerid,SData[Site]);
    TextDrawHideForPlayer(playerid,SData[Stimedraw]);
	TextDrawShowForPlayer(playerid,SData[Site]);
    TextDrawShowForPlayer(playerid,SData[Stimedraw]);
    for(new i=0;i<3;i++) TextDrawHideForPlayer(playerid,Logo[i]),TextDrawShowForPlayer(playerid,Logo[i]);
    for(new i=0;i<5;i++) TextDrawHideForPlayer(playerid,DamageTD[i]),TextDrawShowForPlayer(playerid,DamageTD[i]);
    }
    }
    if(PlayerInfo[playerid][pGang] != 0)
	{
	if(PlayerInfo[playerid][pGJob] != 0 && PlayerInfo[playerid][pGLvl] < 2) GivePlayerSpec(playerid, 4);
	if(Gang[PlayerInfo[playerid][pGang]] != 1)
	{
    PlayerInfo[playerid][pGang] = 0;
    PlayerInfo[playerid][pGLvl] = 0;
    PlayerInfo[playerid][pGSkin] = 0;
    PlayerInfo[playerid][pGJob] = 0;
    }
    }
    return 1;
}

