            for (new t; t < 3; t++)
            {
            if(pDynamiteObject[playerid][t] != 0)
            {
            DestroyDynamicObject(pDynamiteObject[playerid][t]);
            pDynamiteObject[playerid][t] = 0;
            }
            }
			new veh = GetPlayerVehicleID(playerid);
			new Float:ssx, Float:ssy, Float:ssz;
			if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && PlayerInfo[playerid][pAdmin] == 0 && PlayerInfo[playerid][pVip] < 3)
