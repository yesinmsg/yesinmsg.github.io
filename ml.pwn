    for(new i = 0; i < 13; i++) GetPlayerWeaponData(playerid, i, Weaponsr[playerid][i][0], Weaponsr[playerid][i][1]);
    }
	else
    {
	SetPlayerPos(playerid,Posit[playerid][0],Posit[playerid][1],Posit[playerid][2]); // set his saved position x,y and z.
    SetPlayerFacingAngle(playerid,Posit[playerid][3]); // set his saved facing angle.];
   	SetPlayerInterior(playerid, coord[playerid][0]);
    }
    SetCameraBehindPlayer(playerid); // set the camera..behind the player (looks better).
    SetPVarInt(playerid,"AntiCBug",0);
    SetPlayerWorldBounds(playerid, 20000.0000, -20000.0000, 20000.0000, -20000.0000);
	ResetPlayerCash(playerid);
	ac_ResetPlayerWeapons(playerid);
    full[playerid] = DEBUGWORLD;
    SetPlayerVirtualWorld(playerid,DEBUGWORLD);
    StopAudioStreamForPlayer(playerid);
    TextDrawHideForPlayer(playerid,SData[Site]);
    TextDrawHideForPlayer(playerid,SData[Stimedraw]);
    PlayerTextDrawHide(playerid,PotDamage[playerid]);
	PlayerTextDrawHide(playerid,LVL[playerid]);
	for(new b=0;b<3;b++) TextDrawHideForPlayer(playerid,Logo[b]);
	for(new b=0;b<5;b++) TextDrawHideForPlayer(playerid,DamageTD[b]);
	for(new i=0;i<10;i++) TextDrawHideForPlayer(playerid, GradientText[i]);
	for(new i=0;i<30;i++){SendClientMessage(playerid,0," ");}
   	SendClientMessage(playerid,COLOR_BODY_INTERFACE,"Вы перемещены в облегченный мир. Отключена большая часть настроек и опций потребляющих ваш трафик");
	SendClientMessage(playerid,COLOR_BODY_INTERFACE,"В этом мире вам недоступен общий чат и большая часть команд основного мира. '/LOBBY' - выйти из LiteWorld");
