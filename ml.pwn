Process(BuyWeapon(playerid,weaponid,wname[],ammo)){
	if(PlayerInfo[playerid][pKills] > 0)
	{
	if(GetPlayerCash(playerid) >= GetAmmoCost(weaponid)) GivePlayerCash(playerid,-GetAmmoCost(weaponid));
    else
    {
    new coststr[64];
    AmmoMenu(playerid);
    formatex(coststr,"~r~Not enough Cash!~n~~h~~h~~g~Cost this weapon - %d$",GetAmmoCost(weaponid));
	return GameTextForPlayer(playerid, coststr,3000,3);
	}
	}
	new string[128];
	ac_GivePlayerWeapon(playerid,weaponid,ammo);
	if(ammo > 1)format(string,sizeof string,"Оружие %s с %d патронами куплено за %d $ ",wname,ammo,GetAmmoCost(weaponid));
	else format(string,sizeof string,"Оружие %s куплено за %d $ ",wname,GetAmmoCost(weaponid));
	SendClientMessage(playerid,COLOR_ORANGE,string);
	AmmoMenu(playerid);
	return true;
}

stock NumericCount(const stri[]){
	new str[300],nc[2],l = strlen(str);
	format(str,sizeof str,"%s ",stri);
	for(new i;i<l;i++){
		switch(str[i]){
		case '0'..'9':nc[0] += 1;
		default:if(nc[0]>=1)nc[0]=0,nc[1] += 1;
		}}return nc[1];
}

stock GetVehicleDriver(vehicleid){
	foreach(i){
		if(GetPlayerVehicleID(i) == vehicleid && GetPlayerVehicleSeat(i) == 0)return i;
	}return INVALID_PLAYER_ID;
}


stock SavePosition(playerid){
	if(!XYZPos[playerid][S]){
		GetPlayerPos(playerid,XYZPos[playerid][X],XYZPos[playerid][Y],XYZPos[playerid][Z]);
		GetPlayerFacingAngle(playerid,XYZPos[playerid][A]);
		XYZPos[playerid][I] = GetPlayerInterior(playerid);
		XYZPos[playerid][W] = GetPlayerVirtualWorld(playerid);
		GetPlayerHealth(playerid,XYZPos[playerid][H]);
		GetPlayerArmour(playerid,XYZPos[playerid][AR]);
		XYZPos[playerid][S] = true;
	}
	return true;
}

