            if(PlayerInfo[killerid][pLang] == 2) formatex(string, "Вы получили +%d$ на банк. ячейку за серию убийств на DM/TDM", 1000*KomboKills[killerid]);
			else formatex(string, "You achive +%d$ to the bank cell for a series of killings on DM/TDM", 1000*KomboKills[killerid]);
			SendClientMessage(killerid, 0x00FF00FF, string);
            }
            if(KomboKills[killerid]%2!=0 || KomboKills[killerid] > 13) PlayerPlaySound(killerid, 31205, 0.0, 0.0, 0.0);
			if(KomboKills[killerid] == 3) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~Respect Spree ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] == 5) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~Playa ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] == 7) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~Homie ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] == 9) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~Jacker ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] == 11) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~High Roller ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] == 13) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~Criminal ~r~~h~~h~<", 3000, 6);
			if(KomboKills[killerid] > 13) GameTextForPlayer(killerid, "~r~~h~~h~> ~y~~h~~h~God Killer ~r~~h~~h~<", 3000, 6);
            KillTimer(TimerKillSpree[killerid]);
			TimerKillSpree[killerid] = SetTimerEx("StopKillSpree", 20000, false, "i", killerid);
            }
            //
            if(full[killerid] == 0 && tdmfull[killerid] == 0 && (PlayerInfo[playerid][pGang] != PlayerInfo[killerid][pGang] && PlayerInfo[playerid][pGang] != 0))
			{//
            PlayerInfo[killerid][pSpree]++;
            
            if(bounty[playerid] > 0)
	        {
	        new strr[128],strr2[128];
	        if(MaskEnable[killerid]) formatex(strr, "<Хитлист> Неизвестный выполнил заказ на убийство %s(id:%d). Награда составила %d$.",PlayerInfo[playerid][pName], playerid, bounty[playerid]);
