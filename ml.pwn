stock GangCreate(playerid, name[])
{
if(GetFreeGang() >= MAX_GANGS) return TempDialog(playerid,"{FF0000}-==[ Информация ]==-","Извинините, но кол-во кланов на сервере достинуло max значения","Ок");
for(new i=1; i<MAX_GANGS; i++)
{
if(Gang[i] == 1)
{
if(strfind(GName[i], name, true) != -1) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "Извините, но такое название клана уже занято\r\nвведите другое название\r\nв последующем вы всегда сможете его изменить:", "Принять", "Отмена");
}}
if(!strlen(name)) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "Введите название клана\r\n нельзя оставлять поле пустым\r\nв последующем вы всегда сможете его изменить:", "Принять", "Отмена");
if(strlen(name) < 2) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "Не меньше 2-ух символов в названии:", "Принять", "Отмена");
if(strlen(name) > 16) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "Не больше 12-ти символов в названии:", "Принять", "Отмена");
if(!valid_password(name)) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "Недопустимые символы в названии, используйте A-z 0-9:", "Принять", "Отмена");
if(emptyMessage(name)) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "В названии клана обнаружены пробелы:", "Принять", "Отмена");
       	new count;
        for (new i; i < strlen(name); i++)
        {
        if (name[i] >= '0' && name[i] <= '9')
        {
        count++;
        if(count >= 6) return ShowPlayerDialog(playerid, 2002, DIALOG_STYLE_INPUT, "{FF0000} Clan Create", "В названии слишком много цифр, не больше 6 цифр:", "Принять", "Отмена");
		}}
spaceGroupsToSpaces(name);
new string[512];
new year,month,day,lolo[128]; getdate(year, month, day);
PlayerInfo[playerid][pGLvl] = 6;
PlayerInfo[playerid][pGang] = GetFreeGang();
format(GName[PlayerInfo[playerid][pGang]], 64, "%s", name);
format(lolo, sizeof(lolo),"%d/%d/%d",day, month, year);
gangsall=gangsall+1;
Gang[GetFreeGang()] = 1;
formatex(string, "{FFFF00}Ваш клан успешно создан!\r\n{FFFF00}Название клана:{FFFFFF} %s (id:%d)\r\n{FFFF00}Дата основания: {FFFFFF}%s\r\n{FFFF00}Создатель клана: {FFFFFF}%s(%d)\r\nУправление кланом '/menu' -> меню клана -> пункт редактировать\r\n{62FF62}Принимай игроков и увеличивай влияние клана. Удачи!", GName[PlayerInfo[playerid][pGang]],PlayerInfo[playerid][pGang], lolo, PlayerInfo[playerid][pName], playerid);
TempDialog(playerid,"{FF0000}-==[ Информация ]==-",string,"Ок");
CreateClan(PlayerInfo[playerid][pGang]);
return 1;
}

