		}
    }
    }
   	if(RopeObject[playerid] != 0 || MagnetObject[playerid] != 0)
	{
        KillTimer(AttachVehicleTimer[playerid]);
        if(RopeObject[playerid] != 0)
        {
        DestroyObject(RopeObject[playerid]);
        RopeObject[playerid] = 0;
        }
        if(MagnetObject[playerid] != 0)
        {
        DestroyObject(MagnetObject[playerid]);
        MagnetObject[playerid] = 0;
        }
        SetPVarInt(playerid,"Rope",0);
	}
	if(PlayerInfo[playerid][pSetting][1] == 0) R_Vehicle[playerid] = VehicleSpeed_1;
	StopDrift(playerid);
	if(DriftTimer[playerid] > 0)KillTimer(DriftTimer[playerid]);
	HideAttack(playerid);
	for(new x = 0; x < 3; x++) DestroyDynamic3DTextLabel(MyInfo[playerid][x]);
	if(playertimer[playerid] != 0) KillTimer(playertimer[playerid]),playertimer[playerid] = 0;
	PlayerTextDrawHide(playerid,LVL[playerid]);
	PlayerTextDrawDestroy(playerid,LVL[playerid]);
	DamageTimer[playerid] = 0;
	DestroyPlayerCorpses(playerid);
    SetPVarInt(playerid,"AntiCBug",ServerInfo[AntiCBug]);
    KillTimer(DamageTimer[playerid]);
    PlayerTextDrawHide(playerid,PotDamage[playerid]);
    PlayerTextDrawDestroy (playerid,PotDamage[playerid]);
	KillTimer(BlipTimer[playerid]);
	KillTimer(PlayerInfo[playerid][TimerBugC]);
	DestroyMoneyPickups(playerid);
    KillTimer(CountDuelTimer[playerid]);
    KillTimer(TimerKillSpree[playerid]);
    dmetka[playerid] = false;
    DestroyDynamic3DTextLabel(MyInfo2[playerid]);
    KillTimer(healtimer[playerid]);
    if(playerid == VotingKick)
	{
    formatex(string, "[VOTE]: Игрок покинул сервер, опрос автоматически удален");
    SendClientMessageToAllEx(COLOR_YELLOW, string);
    new plip[32],myip,query[124];
    GetPVarString(VotingKick, "saveip", plip, sizeof(plip));
    myip=ip2int(plip);
