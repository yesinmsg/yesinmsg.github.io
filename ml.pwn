stock NewRandomSpawn(Float:newx, Float:newy, &playerid=-1)
{
            new Float:temppos[3],Float:angle;
            if(!planeobj)planeobj = CreateObject(1681, 0, 0, 250, 0.0, 0.0, 0.0);
            if(!cameraplaneobj)
			{
			cameraplaneobj = CreateObject(19300, 0, 0, 0, 0.0, 0.0, 0.0);
			AttachObjectToObject(cameraplaneobj, planeobj, 0.0, 0.0, 30, 0.0, 0.0, 0.0, 1);
            }
            SetObjectFacePoint(planeobj, newx, newy);
            GetObjectRot(planeobj, temppos[0], temppos[1], angle);
            MoveObject(planeobj, newx + (1 * floatsin(-angle, degrees)), newy + (1 * floatcos(-angle, degrees)), 315, 40.0);
            GetObjectPos(cameraplaneobj, temppos[0], temppos[1], temppos[2]);
			if(playerid != -1 && cameraplaneobj)
			{
			PData[playerid][spawned] = false;
			TogglePlayerControllable(playerid,false);
	        SetPlayerVirtualWorld(playerid,playerid+1);
			SetPlayerCameraPos(playerid, temppos[0], temppos[1], temppos[2]+30);
			SetPlayerCameraLookAt(playerid, temppos[0], temppos[1],0);
			Streamer_Update(playerid);
			AttachCameraToObject(playerid, cameraplaneobj);
			if(playertimer[playerid] != 0) KillTimer(playertimer[playerid]),playertimer[playerid] = 0;
			playertimer[playerid] = SetTimerEx("RandomSpawnPlayer", 65000, 0, "i", playerid);
			if(PlayerInfo[playerid][pLang] == 2) ShowPlayerDialog(playerid,JUMPPLAYER,DIALOG_STYLE_MSGBOX,""COL_RED"Пилот",""COL_WHITE"Нажмите кнопку 'Прыгнуть' чтобы выпрыгнуть из самолета\nЧерез минуту бездействия вы будете выброшены автоматически","Прыгнуть","");
            else ShowPlayerDialog(playerid,JUMPPLAYER,DIALOG_STYLE_MSGBOX,""COL_RED"Pilot",""COL_WHITE"Press key 'To Jump' for jump from the plane\nAfter a minute of inaction, you will be thrown automatically","To Jump","");
			}
}

stock SetObjectFacePoint(objectid, Float: Xz, Float: Yz) // By Lorenc_ | http://forum.sa-mp.com/showpost.php?p=1456045&postcount=2563
{
    static
        Float: pX,      Float: oX,
        Float: pY,      Float: oY,
        Float: oZ
    ;
    GetObjectRot(objectid, oX, oY, oZ);
    GetObjectPos(objectid, pX, pY, oZ);

    oZ = ( floatadd(atan2(floatsub(Yz, pY), floatsub(Xz, pX)), 270.0) );

    SetObjectRot(objectid, oX, oY, oZ);
}
