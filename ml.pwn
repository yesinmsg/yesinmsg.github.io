public DestroyMoneyPickups(playerid)
{
    if(PickupsCreated[playerid] == true)
    {
	for(new n=0; n<DropLimit; n++)
	{
	    if(MoneyPickup[playerid][n] != 0)
		{
		DestroyPickup(MoneyPickup[playerid][n]);
		MoneyPickup[playerid][n] = 0;
		}
	}
	PickupsCreated[playerid] = false;
	}
}


stock strrest(const str[], &index){
	new length = strlen(str);while ((index < length) && (str[index] <= ' ')){
		index++;}new offset = index;new result[128];
	while ((index < length) && ((index - offset) < (sizeof(result) - 1))){
		result[index - offset] = str[index];index++;}
	result[index - offset] = EOS;return result;
}


