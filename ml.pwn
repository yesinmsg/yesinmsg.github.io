public fixbugmodellist(playerid, model)
{
switch (model)
{
case 1: ShowModelSelectionMenu(playerid, vehlist, "Vehicles - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
case 2: ShowModelSelectionMenu(playerid, motolist, "Moto - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
case 3: ShowModelSelectionMenu(playerid, planelist, "Planes - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
case 4: ShowModelSelectionMenu(playerid, heliclist, "Helicopters - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
case 5: ShowModelSelectionMenu(playerid, boatlist, "Boats - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
case 6: ShowModelSelectionMenu(playerid, skinlist, "All Skin - Click To Spawn",0x2C2727AA,0x33CCFFFF,0x235AFFFF);
}
}

public OnPlayerModelSelection(playerid, response, listid, modelid)
{
    if(listid == vehlist)
    {
        if(response)
        {
		    DeliveryCarStep(playerid, modelid);
        }
        else DeliveryCars(playerid);
        return 1;
    }
    if(listid == motolist)
    {
        if(response)
        {
		    DeliveryCarStep(playerid, modelid);
        }
        else DeliveryCars(playerid);
        return 1;
    }
    if(listid == planelist)
    {
        if(response)
        {
		    DeliveryCarStep(playerid, modelid);
        }
        else DeliveryCars(playerid);
        return 1;
    }
    if(listid == heliclist)
    {
        if(response)
        {
		    DeliveryCarStep(playerid, modelid);
        }
        else DeliveryCars(playerid);
        return 1;
    }
    if(listid == boatlist)
    {
        if(response)
        {
		    DeliveryCarStep(playerid, modelid);
        }
        else DeliveryCars(playerid);
        return 1;
    }
   	if(listid == skinlist)
	{
	    if(response) {
	    new str[56];
	    format(str,sizeof str,"Скин: %d установлен",modelid);
	    SendClientMessage(playerid,COLOR_SYS,str);
	    SendClientMessage(playerid,COLOR_SYS,"/ms - сохранить этот скин. /rms - удалить сохраненный скин");
	    SetPlayerSkin(playerid,modelid);
		}
	    else ShowPlayerDialog(playerid, OTHERSKIN1, DIALOG_STYLE_LIST, "{FF0000}Choose Skin", "{FFA700}ID Skin\n{FFA700}Preview Skin", "Ok", "Cancel");
    	return 1;
	}
    return 1;
}
