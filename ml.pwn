Process(Float:frandomof(Float:mini, Float:maxi)){
	new Float:minex = fmin(mini, maxi);
	new Float:maxex = fmax(mini, maxi);
	new Float:mix=((maxex - minex)/1000);
	return minex+(mix*random(1000));
}

Process(Float:fmin(Float:v1, Float:v2)){
	if(v1<v2)return v1;
	else return v2;
}

Process(Float:fmax(Float:v1, Float:v2)){
	if(v1>v2)return v1;
	else return v2;
}

