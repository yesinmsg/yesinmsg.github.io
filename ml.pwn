  if( Posz[0] >= ZoneInfo[i][E_ZONEMINX] && Posz[0] <= ZoneInfo[i][E_ZONEMAXX] && Posz[1] >= ZoneInfo[i][E_ZONEMINY] && Posz[1] <= ZoneInfo[i][E_ZONEMAXY] ) return 1;
