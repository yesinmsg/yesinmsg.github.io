    SendClientMessageToAllEx(COLOR_RED,msgcheat);
    RemovePlayerWeapon(playerid, weaponid);
    SaveToFile("KickLog",msgcheat);
    Kick(playerid);
    return 1;
    }
    formatex(msgcheat,"Игрок %s (ID: %d) подозревается в читинге оружия: %s.",PlayerInfo[playerid][pName],playerid,WeaponNames[weaponid]);
    MessageToAdmins(GREEN,msgcheat);
    RemovePlayerWeapon(playerid, weaponid);
