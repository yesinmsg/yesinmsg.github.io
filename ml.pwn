}

forward Processor();
public Processor()
{
   foreach(i)
   {
    	if(PData[i][spawned])
		{
        if(GetPVarInt(i, "PlayerInAFK") == 0) SetPVarInt(i, "PlayerInAFK", -1);
        else if(GetPVarInt(i, "PlayerInAFK") == -1) SetPVarInt(i, "PlayerInAFK", 1);
        else if(GetPVarInt(i, "PlayerInAFK") > 0)
        {
        new strd[56];
        SetPVarInt(i, "PlayerInAFK", GetPVarInt(i, "PlayerInAFK")+1);
        new seconds = GetPVarInt(i, "PlayerInAFK"),minets = 0;
        if(seconds >= 60)while(seconds > 59){seconds -= 60;minets += 1;}
        formatex(strd, "AFK: [%d:%02d]",minets,seconds);
        if(GetPVarInt(i, "PlayerInAFK") > 4) SetPlayerChatBubble(i, strd, 0x33AAFFFF, 100.0, 1200);
        if(GetPVarInt(i, "PlayerInAFK") > 4 && GetPVarInt(i, "checksetname") == 0)
        {
        SetPVarInt(i, "checksetname", 1);
        format(strd,MAX_PLAYER_NAME,"AFK_%s", PlayerInfo[i][pName]);
        SetPlayerName(i,strd);
        }
   	    }
        //
        if(PlayerInfo[i][pSetting][0] == 0)
        {
		new ping = GetPlayerPing(i);
		new Float:packetloss = NetStats_PacketLossPercent(i);
		new string[256];
        formatex(string, "~g~~h~Ping: ~w~%d/~r~%d  ~g~~h~FPS: ~w~%d  ~g~~h~PL: ~w~%.1f%%  ~g~~h~KDA: ~w~%0.2f  ~g~~h~Spree: ~w~%d", ping,ServerInfo[MaxPing],pFPS[i],packetloss,GetPVarFloat(i, "PlayerKDA"),PlayerInfo[i][pSpree]);
		PlayerTextDrawSetString(i,LVL[i],string);
        }
        if(PlayerInfo[i][God] == true) SetPlayerHealth(i,99999);
        //
        if(IsPlayerInAnyVehicle(i) && GetPlayerState(i) == PLAYER_STATE_DRIVER)
        {
        if (ServerInfo[Overload] == 1)
        {
		if(PlaneVeh(GetPlayerVehicleID(i)))
 		{
 		new Float:pospl[3];
 		GetVehiclePos(GetPlayerVehicleID(i), pospl[0], pospl[1], pospl[2]);
 		if(abs(floatround(GetPVarFloat(i, "PlaneZ") - pospl[2])) >= 20 && GetPVarFloat(i, "PlaneZ") != 0)
        {
        GameTextForPlayer(i,"~r~Alert~n~ ~r~Overload", 1500, 6);
		if(GetPlayerDrunkLevel(i) < 10000) SetPlayerDrunkLevel(i, GetPlayerDrunkLevel(i)+2000);
		}
		else if(GetPlayerDrunkLevel(i) >= 2000) SetPlayerDrunkLevel(i, GetPlayerDrunkLevel(i)-2000);
		SetPVarFloat(i, "PlaneZ", pospl[2]);
        }
		else
		{
		if(GetPlayerDrunkLevel(i) > 0 && GetPVarFloat(i, "PlaneZ") != 0) SetPlayerDrunkLevel(i, 0);
		SetPVarFloat(i, "PlaneZ", 0);
		}
        }
		}
        if(PlayerInfo[i][GodCar] == true && IsPlayerInAnyVehicle(i))
		{
		new Float:fVehicleHealth;
	    GetVehicleHealth(GetPlayerVehicleID(i),fVehicleHealth);
	    if(fVehicleHealth < 1000 && fVehicleHealth > 250) SetVehicleHealth(GetPlayerVehicleID(i),1000),RepairVehicle(GetPlayerVehicleID(i));
	    else if(fVehicleHealth <= 250)
        {
        if(IsVehicleUpsideDown(GetPlayerVehicleID(i)) && !BoatVeh(GetPlayerVehicleID(i)) && !PlaneVeh(GetPlayerVehicleID(i)) && !BikeVeh(GetPlayerVehicleID(i)) && !MotoVeh(GetPlayerVehicleID(i)) && !IsVehicleToy(GetPlayerVehicleID(i)))
		{
        new Float:pods[4];
        GetVehiclePos(GetPlayerVehicleID(i),pods[0],pods[1],pods[2]);
        GetVehicleZAngle(GetPlayerVehicleID(i),pods[3]);
        SetVehiclePos(GetPlayerVehicleID(i),pods[0],pods[1],pods[2]+1);
        SetVehicleZAngle(GetPlayerVehicleID(i),pods[3]);
        }
        SetVehicleHealth(GetPlayerVehicleID(i),1000),RepairVehicle(GetPlayerVehicleID(i));
		}
        }
        //
   }//
   }//
   new days, months, years;
   gettime(SData[hour],SData[minet],SData[second]);
   getdate(years, months, days);
   if(SData[minet] == 15 && SData[second] == 1) ChoseWorldWeather();
