			SendClientMessage(playerid,orange,string);
			formatex(string," Игровых часов: %s  Убил: %d  Смертей: %d  Коэфиц.: %0.2f  Админ уровень: %d", ConvertSeconds(floatround(GetPVarInt(player1, "TimesOnServer")+NetStats_GetConnectedTime(player1)*0.001)), PlayerInfo[player1][pKills], PlayerInfo[player1][pDeaths], Float:PlayerInfo[player1][pKills]/Float:PlayerInfo[player1][pDeaths], PlayerInfo[player1][pAdmin] );
			SendClientMessage(playerid,COLOR_ORANGE,string);
