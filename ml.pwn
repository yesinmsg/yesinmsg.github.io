stock UnloadMTAResources(arayname[]){
	new buffer[100];
	formatex(buffer,MAPFOLDER,arayname);
	if(!fexist(buffer))return printf("File %s not found. Cancel unload map",arayname);
	new File:read  = fopen(buffer,io_read);
	if(!read)return false;
	new count[2],id;
	while(fread(read,buffer)){
		if(buffer[0] == 'o'){
			id = strval(buffer[1]);
			if(!id)continue;
			DestroyDynamicObject(id);
			count[0]++;
		}
		if(buffer[0]=='v'){
			id = strval(buffer[1]);
			if(!id)continue;
			DestroyVehicle(id);
			count[1]++;
		}
	}
	fclose(read);
	printf("File %s unloaded |obj:%d|veh:%d|",arayname,count[0],count[1]);
	formatex(buffer,MAPFOLDER,arayname);
	fremove(buffer);
	return true;
}


stock LoadMTAResources(source[],bool:compile=false){
	if(!fexist(source))return printf("File %s was not founded",source);
	new File:load = fopen(source,io_read);
	new File:output = fopen("output.txt",io_append);
	new buffer[512],operate[256],coment[256];
	new Float:px,Float:py,Float:pz;
	new Float:rx,Float:ry,Float:rz;
	new model,idx,object_count=0,vehicle_count=0,remove_count=0;
	new objectz,vehiclez,remobjz; //remobjz;
	new buffer2[100];
	formatex(buffer2,MAPFOLDER,source);
	if(compile){
		formatex(buffer,"// Compile %s start \r\n",source);
		fwrite(output,buffer);
	}
	if(ExistObjects(source))UnloadMTAResources(source);
	new File:write = fopen(buffer2,io_write);
	while(fread(load,buffer)){
		idx = strfind(buffer,"<")+1;
		operate = strtok(buffer,idx);
		if(Command(operate,"object")){
			object_count++;
			idx = 4+strfind(buffer,"id=\"");
			coment = strtok(buffer,idx,'\"');
			idx = 7+strfind(buffer,"model=\"");
			model = strval(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posX=\"");
			px = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posY=\"");
			py = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posZ=\"");
			pz = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"rotX=\"");
			rx = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"rotY=\"");
			ry = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"rotZ=\"");
			rz = floatstr(strtok(buffer,idx,'\"'));
			if(!compile){objectz = CreateDynamicObject(model,px,py,pz,rx,ry,rz, LOBBYWORLD, 0, -1, 300);
				formatex(buffer,"o%d \r\n",objectz);
				fwrite(write,buffer);
			}else{formatex(buffer,"CreateDynamicObject(%d,%f,%f,%f,%f,%f,%f); // %s\r\n",model,px,py,pz,rx,ry,rz,coment);
				fwrite(output,buffer);
			}
		}
		if(compile){
		if(Command(operate,"removeWorldObject")){
			remove_count++;
			idx = 4+strfind(buffer,"id=\"");
			coment = strtok(buffer,idx,'\"');
			idx = 7+strfind(buffer,"model=\"");
			model = strval(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posX=\"");
			px = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posY=\"");
			py = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posZ=\"");
			pz = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"radius=\"");
			rx = floatstr(strtok(buffer,idx,'\"'));
			if(!compile){foreach(i) remobjz = RemoveBuildingForPlayer(i, model,px,py,pz,rx);
				formatex(buffer,"r%d \r\n",remobjz);
				fwrite(write,buffer);
			}else{formatex(buffer,"RemoveBuildingForPlayer(playerid, %d, %f,%f,%f,%f); // %s\r\n",model,px,py,pz,rx,coment);
				fwrite(output,buffer);
			}
		}}
		if(Command(operate,"vehicle")){
			vehicle_count++;
			idx = 4+strfind(buffer,"id=\"");
			coment = strtok(buffer,idx,'\"');
			idx = 7+strfind(buffer,"model=\"");
			model = strval(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posX=\"");
			px = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posY=\"");
			py = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"posZ=\"");
			pz = floatstr(strtok(buffer,idx,'\"'));
			idx = 6+strfind(buffer,"rotZ=\"");
			rz = floatstr(strtok(buffer,idx,'\"'));
			if(!compile){vehiclez = AddStaticVehicle(model,px,py,pz,rz,random(126),random(126));
				formatex(buffer,"v%d \r\n",vehiclez);
				fwrite(write,buffer);
			}else{formatex(buffer,"AddStaticVehicle(%d,%f,%f,%f,%f,random(126),random(126)); // %s\r\n",model,px,py,pz,rz,coment);
				fwrite(output,buffer);
			}
		}
	}
	if(!compile){
		printf("File %s loaded |obj:%d|veh:%d|robj:%d",source,object_count,vehicle_count,remove_count);
	}else{
		if(object_count > 0)formatex(buffer,"// Total objects %d\r\n",object_count);fwrite(output,buffer);
		if(vehicle_count > 0)formatex(buffer,"// Total vehicles %d\r\n",vehicle_count);fwrite(output,buffer);
		if(remove_count > 0)formatex(buffer,"// Total remove objects %d\r\n",remove_count);fwrite(output,buffer);
		printf("File %s compilled |obj:%d|veh:%d|",source,object_count,vehicle_count,remove_count);
	}
	fclose(load);
	fclose(write);
	fclose(output);
	return true;
}



