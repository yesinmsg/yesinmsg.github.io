    {
    SetPVarInt(playerid,"PRISON",0);
    DestroyDynamicObject(pNetTrapObject[playerid]);
    pNetTrapObject[playerid] = 0;
    }
	SetPVarInt(playerid, "PlayerInAFK", -2);
	KillTimer(PlayerInfo[playerid][TimerBugC]);
	StopDrift(playerid);
	if(healtimer[playerid] != 0) StopHealPlayer(playerid);
	if (full[playerid] == 0 && tdmfull[playerid] == 0 && PlayerInDuel[playerid] == 0 && onexone[playerid] == 0)
	{
	if(MaskEnable[playerid])
	{
	MaskEnable[playerid] = false;
	PlayerInfo[playerid][pMask] = 0;
	if(PlayerInfo[playerid][pLang] == 2) SendClientMessage(playerid, 0x00FFFFFF, "Маска повреждена, купите новую в оружейном блоке.");
    else SendClientMessage(playerid, 0x00FFFFFF, "Your mask is broken, buy new on weapon shop.");
	foreach(i) ShowPlayerNameTagForPlayer(i, playerid, 1);
	if(PlayerInfo[playerid][pGang] != 0 && PlayerInfo[playerid][pGJob] == 3 || PlayerInfo[playerid][pVip] >= 3 || PlayerInfo[playerid][pAdmin] > 0) SetPlayerColourAlpha(playerid, 170);
	}
	new money = GetPlayerCash(playerid);
	if (money > suecost) GivePlayerCash(playerid,-floatround(money*0.15)),DropMoney(playerid, floatround(money*0.15));
	if(!IsPlayerInAnyVehicle(playerid)) CreatePlayerCorpses(INVALID_PLAYER_ID, playerid, 15);
	DropHealth(playerid);
	ac_ResetPlayerWeapons(playerid);
    }
	if(tdmfull[playerid] != 0) foreach(i) if (GetPlayerTeam(i) == GetPlayerTeam(playerid) && tdmfull[i] != 0) SetPlayerMarkerForPlayer(i, playerid, COLOR_RED);
    if(PlayerInDuel[playerid] == 1)
    {
    DuelPlaceVisit = false;
    PlayerInDuel[playerid] = 0;
    GetDuelCreate[playerid] = INVALID_PLAYER_ID;
    GameTextForPlayer(playerid,"~r~LOSE ROUND",3000,3);
    SetPVarInt(playerid,"AntiCBug",ServerInfo[AntiCBug]);
	foreach(i) if(PlayerInDuel[i] == 1) DuelRezult(i,playerid);
    }
    if(onexone[playerid] != 0)
	{
            if(killerid != INVALID_PLAYER_ID)
            {
		    if(onexone[killerid] == 1 || onexone[killerid] == 3) SetPlayerHealth(killerid, 100);
		    else SetPlayerHealth(killerid, 100),SetPlayerArmour(killerid, 100);
		    SetPlayerChatBubble(killerid,"Здоровье восстановлено",0x235AFFFF,25.0,8000);
            PlayerInfo[killerid][pDuels]++;
            GameTextForPlayer(killerid,"~g~WIN ROUND",3000,3);
            }
	    	new Float:stpos[3],Float:lastpos[2];
            GetXYInFrontOfPlayer(playerid, lastpos[0], lastpos[1], -4);
            GetPlayerPos(playerid, stpos[0], stpos[1], stpos[2]);
            SetPlayerCameraPos(playerid, lastpos[0], lastpos[1], stpos[2] +3);
            SetPlayerCameraLookAt(playerid, stpos[0], stpos[1], stpos[2]);
            GameTextForPlayer(playerid,"~r~LOSE ROUND",3000,3);
    }
   	if(killerid == INVALID_PLAYER_ID)
    {
    SendDeathMessage(INVALID_PLAYER_ID,playerid,reason);
    GameTextForPlayer(playerid, "~r~~h~~h~ wasted", 3000, 0);
	}
	else
	{
    SetPVarInt(playerid,"K_Times",GetPVarInt(playerid,"K_Times") + 1);
    if(GetPVarInt(playerid,"K_Times") > 2 && IsPlayerConnected(playerid))
	{
         formatex(stringC, "Игрок %s (ID: %d) был кикнут за использование чита: fakekill *", PlayerInfo[playerid][pName],playerid);
         SendClientMessageToAllEx(COLOR_RED, stringC);
         Kick(playerid);
	     return 1;
	}
	roundkills++;// убито всего
   	        if(!MaskEnable[killerid]) SendDeathMessage(killerid,playerid,reason);
            else SendDeathMessage(INVALID_PLAYER_ID,playerid,255);
			foreach(i) if(i != killerid && i != playerid) PlayerSoundForAll(i,32402);
			GetWeaponName(reason, gunname, sizeof(gunname));
            if(PlayerInfo[killerid][pLang] == 2) formatex(string, "* Ты убил игрока %s(id:%d) {FFF8DC}(оружием: %s), c дистанции: %.3f",PlayerInfo[playerid][pName], playerid, gunname, GetDistanceBetweenPlayers(playerid, killerid));
			else formatex(string, "* You kill %s(id:%d) {FFF8DC}(weapon: %s), distance: %.3f",PlayerInfo[playerid][pName], playerid, gunname, GetDistanceBetweenPlayers(playerid, killerid));
			SendClientMessage(killerid, COLOR_WHITE, string);
			if(PlayerInfo[playerid][pLang] == 2)
			{
		    if(MaskEnable[killerid]) formatex(string, "* Тебя убил неизвестный {FFF8DC}(оружием: %s), c дистанции: %.3f",gunname, GetDistanceBetweenPlayers(playerid, killerid));
			else formatex(string, "* Тебя убил %s(id:%d) {FFF8DC}(оружием: %s), c дистанции: %.3f",PlayerInfo[killerid][pName], killerid, gunname, GetDistanceBetweenPlayers(playerid, killerid));
			}
			else
			{
			if(MaskEnable[killerid]) formatex(string, "* You killed by unknown {FFF8DC}(weapon: %s), distance: %.3f",gunname, GetDistanceBetweenPlayers(playerid, killerid));
			else formatex(string, "* You killed by %s(id:%d) {FFF8DC}(weapon: %s), distance: %.3f",PlayerInfo[killerid][pName], killerid, gunname, GetDistanceBetweenPlayers(playerid, killerid));
			}
			SendClientMessage(playerid, COLOR_WHITE, string);
		    if(MaskEnable[killerid]) formatex(stringC, "Убит неизвестным");
			else formatex(stringC, "Убит игроком %s", PlayerInfo[killerid][pName]);
            SetPlayerChatBubble(playerid, stringC, 0xFFFFFF00, 500.0, 5000);

            if(PlayerInDuel[killerid] == 0 && onexone[killerid] == 0)
			{//
			KillTimer(TimerKillSpree[playerid]);
			KomboKills[killerid]++;
            KomboKills[playerid] = 0;
            if(KomboKills[killerid] > 1)
			{
			if(full[killerid] > 0 || tdmfull[killerid] > 0)
			{
			if(KomboKills[killerid]%2!=0 || KomboKills[killerid] > 13)
			{
			if (full[killerid] == 4 || full[killerid] == 6 || full[killerid] == 13 || full[killerid] == 14 || tdmfull[killerid] == 6) SetPlayerArmour(killerid, 100);
