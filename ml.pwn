stock GetAroundPlayerVehicleID(playerid, Float:radius = 5.0)
{
    new VehicleID, Float:Posz[3], Float:Distanse;
    GetPlayerPos(playerid, Posz[0], Posz[1], Posz[2]);
    for(new i = MAX_VEHICLES; i != 0; i--)
    {
        Distanse = GetVehicleDistanceFromPoint(i, Posz[0], Posz[1], Posz[2]);
        if(Distanse < radius)
        {
            VehicleID = i;
            Distanse = radius;
        }
    }
    if(!VehicleID) VehicleID = -1;
    return VehicleID;
}

