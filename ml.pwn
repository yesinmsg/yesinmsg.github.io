dcmd_lspecvehicle(playerid,params[]) {
    if(PlayerInfo[playerid][pAdmin] >= 6) {
	    if(!strlen(params)) return SendClientMessage(playerid,red,"[Info] "COL_WHITE"ПРАВКА: /lspecvehicle [id транспорта] (следить за траспортом игрока)");
		new specvehicleid = strval(params);
		if(specvehicleid < MAX_VEHICLES) {
		    GetPlayerPos(playerid,Pos[playerid][0],Pos[playerid][1],Pos[playerid][2]);
			GetPlayerFacingAngle(playerid,Pos[playerid][3]);
			SetPlayerPos(playerid, Pos[playerid][0],Pos[playerid][1],Pos[playerid][2]+3);
