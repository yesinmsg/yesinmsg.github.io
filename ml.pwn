			{
			SyncPlayers[playerid] = true;
			GetPlayerPos(playerid,Posit[playerid][0],Posit[playerid][1],Posit[playerid][2]); // save x,y and z for this player.
			GetPlayerFacingAngle(playerid,Posit[playerid][3]); // save the facing angle for this player.
			coord[playerid][0] = GetPlayerInterior(playerid);
			coord[playerid][1] = GetPlayerVirtualWorld(playerid);
			GetPlayerHealth(playerid, healthp[playerid]);
			if(healthp[playerid] > 100) healthp[playerid] = 100;
			GetPlayerArmour(playerid, armourp[playerid]);
