		{FFFFFF}Онлайн: {AA22EE}%d/%d\n\
		{FFFFFF}Апдейт версия: {AA22EE}%i",SData[sTimeDay],SData[sTimeHour],SData[sTimeMin],SData[sTimeSec],gangsall,sizeof(ZoneInfo),MAX_DM_ARENAS,MAX_TDM_ARENAS,MAX_1X1_ARENAS,onlineplayers,GetMaxPlayers(),VERSIONGM);
		UpdateDynamic3DTextLabelText(InLobby,0xFFFFFFFF,strs);
		for(new i; i<3; i++) UpdateDynamic3DTextLabelText(CivilLobby[i],0xFFFFFFFF,strs);
   return 1;
