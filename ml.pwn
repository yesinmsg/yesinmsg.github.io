forward Float:DistanceCameraTargetToLocation(Float:CamX, Float:CamY, Float:CamZ, Float:ObjXq, Float:ObjYq, Float:ObjZq, Float:FrX, Float:FrY, Float:FrZ);
public Float:DistanceCameraTargetToLocation(Float:CamX, Float:CamY, Float:CamZ, Float:ObjXq, Float:ObjYq, Float:ObjZq, Float:FrX, Float:FrY, Float:FrZ)
{
        new Float:TGTDistance;

        TGTDistance = floatsqroot((CamX - ObjXq) * (CamX - ObjXq) + (CamY - ObjYq) * (CamY - ObjYq) + (CamZ - ObjZq) * (CamZ - ObjZq));

        new Float:tmpX, Float:tmpY, Float:tmpZ;

        tmpX = FrX * TGTDistance + CamX;
        tmpY = FrY * TGTDistance + CamY;
        tmpZ = FrZ * TGTDistance + CamZ;

        return floatsqroot((tmpX - ObjXq) * (tmpX - ObjXq) + (tmpY - ObjYq) * (tmpY - ObjYq) + (tmpZ - ObjZq) * (tmpZ - ObjZq));
}

forward Float:GetPointAngleToPoint(Float:x2, Float:y2, Float:Xz, Float:Yz);
public Float:GetPointAngleToPoint(Float:x2, Float:y2, Float:Xz, Float:Yz)
{

  new Float:DX, Float:DY;
  new Float:angle;

  DX = floatabs(floatsub(x2,Xz));
  DY = floatabs(floatsub(y2,Yz));

  if (DY == 0.0 || DX == 0.0)
  {
    if(DY == 0 && DX > 0) angle = 0.0;
    else if(DY == 0 && DX < 0) angle = 180.0;
    else if(DY > 0 && DX == 0) angle = 90.0;
    else if(DY < 0 && DX == 0) angle = 270.0;
    else if(DY == 0 && DX == 0) angle = 0.0;
  }
  else
  {
    angle = atan(DX/DY);

    if(Xz > x2 && Yz <= y2) angle += 90.0;
    else if(Xz <= x2 && Yz < y2) angle = floatsub(90.0, angle);
    else if(Xz < x2 && Yz >= y2) angle -= 90.0;
    else if(Xz >= x2 && Yz > y2) angle = floatsub(270.0, angle);
  }
  return floatadd(angle, 90.0);
}

