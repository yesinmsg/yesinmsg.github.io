forward countdown();
public countdown()
{
    new text[12];
	if(CountDown <= 1)
	{
	CountDown = -1;
	foreach(i)
	{
	    if(IsPlayerInArea(i,xt-coordradius,xt+coordradius,yt-coordradius,yt+coordradius,zt-coordradius,zt+coordradius))
		{
		PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
		GameTextForPlayer(i,"~g~GO ~y~GO ~p~GO",2000,4);
	    }
	}
	return 1;
	}
	CountDown--;
    format(text,sizeof(text),"~w~%d",CountDown);
    SetTimer("countdown",1000,false);
	foreach(i) if(IsPlayerInArea(i,xt-coordradius,xt+coordradius,yt-coordradius,yt+coordradius,zt-coordradius,zt+coordradius)) GameTextForPlayer(i,text,1000,4), PlayerPlaySound(i, 1056, 0.0, 0.0, 0.0);
	return 1;
}

forward Duel(player1, player2);
public Duel(player1, player2)
{
	if(cdt[player1]==6) {
		GameTextForPlayer(player1,"~p~DYELJ HA4ALACJ...",1000,6); GameTextForPlayer(player2,"~p~DYELJ HA4ALACJ...",1000,6);
	}

	cdt[player1]--;
	if(cdt[player1]==0)
