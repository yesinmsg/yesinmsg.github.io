forward RandomSpawnPlayer(playerid);
public RandomSpawnPlayer(playerid)
{
if(playertimer[playerid] != 0) KillTimer(playertimer[playerid]),playertimer[playerid] = 0;
HideDialog(playerid);
LoadCamPlayer(playerid);
ac_GivePlayerWeapon(playerid, 46, 1);
new Float:temppos[3];
GetObjectPos(planeobj, temppos[0], temppos[1], temppos[2]);
