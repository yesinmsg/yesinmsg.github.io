public DuelRezult(killerid,playerid)
{
       if(PlayerInDuel[killerid] == 1 && killerid != INVALID_PLAYER_ID)
       {
       GameTextForPlayer(killerid,"~g~WIN ROUND",3000,3);
       PlayerInDuel[killerid] = 0;
       DuelPlaceVisit = false;
       PlayerInfo[killerid][pDuels]++;
       GetDuelCreate[killerid] = INVALID_PLAYER_ID;
       PlayerPlaySound(killerid, 31205, 0.0, 0.0, 0.0);
       new string[256];
       if(MoneyDuel != 0) formatex(string, ">>> В дуэли между игроками %s и %s победил игрок %s. Награда составила: %d$", PlayerInfo[killerid][pName], PlayerInfo[playerid][pName], PlayerInfo[killerid][pName], MoneyDuel);
       else formatex(string, ">>> В дуэли между игроками %s и %s победил игрок %s", PlayerInfo[killerid][pName], PlayerInfo[playerid][pName], PlayerInfo[killerid][pName]);
       SendClientMessageToAllEx(COLOR_YELLOW, string);
       ac_ResetPlayerWeapons(killerid);
       SetPlayerPos(killerid,Posit[killerid][0],Posit[killerid][1],Posit[killerid][2]); // set his saved position x,y and z.
       SetPlayerFacingAngle(killerid,Posit[killerid][3]); // set his saved facing angle.];
       SetPlayerInterior(killerid, coord[killerid][0]);
       SetPlayerVirtualWorld(killerid, coord[killerid][1]);
       SetCameraBehindPlayer(killerid); // set the camera..behind the player (looks better).
       SetPlayerArmour(killerid, armourp[killerid]);
       SetPlayerHealth(killerid, healthp[killerid]);
       //ResetPlayerCash(killerid);
       SetPVarInt(killerid,"AntiCBug",ServerInfo[AntiCBug]);
       for(new b = 0; b < 13; b++) ac_GivePlayerWeapon(killerid, Weaponsr[killerid][b][0], Weaponsr[killerid][b][1]);
       //if(MoneyDuel != 0) GivePlayerCash(killerid,Moneyr[killerid]+MoneyDuel*2);
       if(MoneyDuel != 0) GivePlayerCash(killerid,MoneyDuel*2);
       }
}





